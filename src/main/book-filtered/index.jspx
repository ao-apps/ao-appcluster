<jsp:root
	xmlns:ao="http://aoindustries.com/ao-taglib"
	xmlns:core="https://semanticcms.com/core/taglib/"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:section="https://semanticcms.com/section/taglib/"
	version="2.1"
>
	<jsp:directive.page language="java" pageEncoding="UTF-8" session="false" />
	<core:page
		title="@{project.name}"
		description="@{project.description}"
	>
		<core:child book="/ao-appcluster/all" page="/index.jspx" />
		<core:child book="/ao-appcluster/core" page="/index.jspx" />
		<core:child book="/ao-appcluster/csync2" page="/index.jspx" />
		<core:child book="/ao-appcluster/dnsonly" page="/index.jspx" />
		<core:child book="/ao-appcluster/imap" page="/index.jspx" />
		<core:child book="/ao-appcluster/jdbc" page="/index.jspx" />
		<p>
			<ao:out value="${currentPage.description}" />
		</p>
		<section:section label="Project Links">
			<ul>
				<li><core:link page="index.jspx">Project Home</core:link></li>
				<li><ao:a href="https://search.maven.org/#search|gav|1|g:%22@{project.groupId}%22%20AND%20a:%22@{project.artifactId}%22">Maven Central Repository</ao:a></li>
				<li><ao:a href="@{project.scm.url}">GitHub</ao:a></li>
			</ul>
		</section:section>
		<section:section label="Modules">
			<ul>
				<li><core:link book="/ao-appcluster/all" page="/index.jspx" /></li>
				<li><core:link book="/ao-appcluster/core" page="/index.jspx" /></li>
				<li><core:link book="/ao-appcluster/csync2" page="/index.jspx" /></li>
				<li><core:link book="/ao-appcluster/dnsonly" page="/index.jspx" /></li>
				<li><core:link book="/ao-appcluster/imap" page="/index.jspx" /></li>
				<li><core:link book="/ao-appcluster/jdbc" page="/index.jspx" /></li>
			</ul>
		</section:section>
	</core:page>
</jsp:root>
